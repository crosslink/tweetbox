<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="1.0+" codebase="http://www.xs4all.nl/~mnankman/tweetbox/" href="TweetBox.jnlp">
    <information>
        <title>TweetBox</title>
        <vendor>mnankman</vendor>
        <homepage href="http://www.xs4all.nl/~mnankman/tweetbox/"/>
        <description>TweetBox</description>
        <icon kind="splash" href="splash.gif"/>
        <offline-allowed/>
        <shortcut>
            <desktop/>
        </shortcut>
    </information>
    <security>
        <all-permissions/>
    </security>
    <resources>
        <j2se version="1.5+"/>
        <property name="jnlp.packEnabled" value="true"/>
        <property name="jnlp.versionEnabled" value="true"/>
        <extension name="JavaFX Runtime" href="http://dl.javafx.com/1.1/javafx-rt.jnlp"/>
        <jar href="TweetBox.jar" main="true"/>
        <jar href="lib/twitter4j-1.1.5.jar"/>
        <jar href="lib/JFXtras-0.3.jar"/>
    </resources>
    <application-desc main-class="tweetbox.ui.Main"/>
</jnlp>
